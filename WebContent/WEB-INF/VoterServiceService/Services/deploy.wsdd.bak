<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from VoterServiceService WSDL service -->

  <service name="VoterService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Services"/>
      <parameter name="wsdlServiceElement" value="VoterServiceService"/>
      <parameter name="schemaQualified" value="http://Services"/>
      <parameter name="wsdlServicePort" value="VoterService"/>
      <parameter name="className" value="Services.VoterServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="VoterService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="signUp" qname="operNS:signUp" xmlns:operNS="http://Services" soapAction="" >
        <parameter qname="pns:nom" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:prenom" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:tel" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:email" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:cin" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:username" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="signIn" qname="operNS:signIn" xmlns:operNS="http://Services" returnQName="retNS:signInReturn" xmlns:retNS="http://Services" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mdp" xmlns:pns="http://Services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="signIn signUp"/>

  </service>
</deployment>
